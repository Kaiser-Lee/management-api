package com.iwhalecloud.retail.oms.properties;

import org.springframework.boot.context.properties.ConfigurationProperties;
import org.springframework.stereotype.Component;

/**
 * ROP参数配置类
 * @author Z
 * @date 2018/10/8
 */
@Component
@ConfigurationProperties(prefix="rop")
public class RopProperties {

    private String platUrl;

    private String zfUrl;

    public String getPlatUrl() {
        return platUrl;
    }

    public void setPlatUrl(String platUrl) {
        this.platUrl = platUrl;
    }

    public String getZfUrl() {
        return zfUrl;
    }

    public void setZfUrl(String zfUrl) {
        this.zfUrl = zfUrl;
    }
}
