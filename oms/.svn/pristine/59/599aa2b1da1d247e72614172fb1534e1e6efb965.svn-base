            return ResultVO.error(resp.getError_msg());
        }

        ResultVO resultVO = new ResultVO();
        resultVO.setResultCode(resp.getError_code());
        resultVO.setResultMsg(resp.getError_msg());
        resultVO.setResultData(resp);
        Map objMap = new HashMap();
