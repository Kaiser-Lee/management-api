package com.iwhalecloud.retail.oms.dto.resquest;

import com.iwhalecloud.retail.oms.entity.*;

import java.io.Serializable;

public class ContentAddReq implements Serializable {

    //内容基础信息
    private ContentBase contentBase;

    //内容标签
    private ContentTag contentTag;

    //内容素材
    private ContentMaterial contentMaterial;

    //软文
    private ContentText contentText;

    //单图片
    private ContentPic contentPic;

    //轮播图
    private ContentOrderpic contentOrderpic;

    //推广图集
    private ContentPicset contentPicset;

    //纯视频
    private ContentVideosDefaultContent contentVideosDefaultContent;

    //视频二级内容
    private ContentVediolv2 contentVediolv2;

    public ContentBase getContentBase() {
        return contentBase;
    }

    public void setContentBase(ContentBase contentBase) {
        this.contentBase = contentBase;
    }

    public ContentTag getContentTag() {
        return contentTag;
    }

    public void setContentTag(ContentTag contentTag) {
        this.contentTag = contentTag;
    }

    public ContentMaterial getContentMaterial() {
        return contentMaterial;
