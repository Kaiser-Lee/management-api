    public void setContentText(ContentText contentText) {
        this.contentText = contentText;
    }

    public ContentPic getContentPic() {
        return contentPic;
    }

    public void setContentPic(ContentPic contentPic) {
        this.contentPic = contentPic;
    }

    public ContentOrderpic getContentOrderpic() {
        return contentOrderpic;
    }

    public void setContentOrderpic(ContentOrderpic contentOrderpic) {
        this.contentOrderpic = contentOrderpic;
    }

    public ContentPicset getContentPicset() {
        return contentPicset;
